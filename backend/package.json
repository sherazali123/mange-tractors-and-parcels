    "ts-node": "^8.10.2",
    "typeorm": "^0.2.24",
    "typescript": "^3.8.3",
    "uuid": "^7.0.1"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "yarn build && node dist/index.js",
    "start:withData": "yarn reset:db && node dist/index.js",
    "build": "rimraf ./dist && tsc -p . && cpx ./src/**/*.graphql ./dist",
    "sync:db": "typeorm schema:sync",
    "migrations:generate": "typeorm migration:generate -n",
    "migration:create": "typeorm migration:create -n",
    "migrations:run": "yarn build && typeorm migration:run",
    "migrations:revert": "typeorm migration:revert",
    "clear:db": "yarn build && node ./dist/database/resetDb.js",
    "reset:db": "yarn 'clear:db' && yarn migrations:run && node ./dist/database/seed.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
