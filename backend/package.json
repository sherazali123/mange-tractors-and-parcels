    "@types/express": "^4.17.2",
    "@types/find": "^0.2.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.7",
    "apollo-server-express": "^2.10.1",
    "bcryptjs": "^2.4.3",
    "camelcase-keys": "^6.2.2",
    "class-validator": "^0.11.0",
    "cpx": "^1.5.0",
    "crypto": "^1.0.1",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "find": "^0.3.0",
    "graphql": "^14.6.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "merge-graphql-schemas": "^1.7.6",
    "moment": "^2.24.0",
